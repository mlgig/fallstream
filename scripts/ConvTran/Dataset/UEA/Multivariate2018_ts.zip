    <![endif]-->
    <script>
	$(window).load(function(){
	(function() {
            $('form > input').keyup(function() {

                var empty = false;
                $('form > input').each(function() {
                        if ($(this).val() == '') {
                                empty = true;
                        }
                });

                if (empty) {
                        $('#submit').attr('disabled', 'disabled');
                } else {
                        $('#submit').removeAttr('disabled');
                }
            });
	})()
	});
	</script>

</head>
